{
  "name": "ats_num_frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "vite": "vite",
    "dev": "run-p vite tailwind:watch",
    "build": "run-p type-check \"build-only {@}\" tailwind:build --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "tailwind:build": "npx tailwindcss -i src/css/style_init.css -o src/css/styles.css",
    "tailwind:watch": "npx tailwindcss -i src/css/style_init.css -o src/css/styles.css --watch"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.17",
    "axios": "^1.13.2",
    "tailwindcss": "^4.1.17",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "npm-run-all2": "^8.0.4",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  }
}
